{
  "name": "gitlog",
  "version": "4.0.8",
  "description": "Git log parser for Node.JS",
  "module": "dist/gitlog.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "pretest": "npm run lint",
    "lint": "tsdx lint src test",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/domharrington/node-gitlog.git"
  },
  "keywords": [
    "git",
    "log",
    "parser"
  ],
  "author": "Dom Harrington <domharrington@protonmail.com>",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 10.x"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^10.46.0",
    "@auto-it/first-time-contributor": "^10.46.0",
    "@auto-it/pr-body-labels": "^10.46.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.1",
    "auto": "^10.46.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "tsdx": "^0.13.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "tslib": "^2.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,json}": "npm run format"
  },
  "auto": {
    "plugins": [
      "npm",
      "released",
      "first-time-contributor",
      "all-contributors",
      "pr-body-labels"
    ]
  }
}
