{
  "name": "gitlog",
  "version": "5.0.0",
  "description": "Git log parser for Node.JS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/domharrington/node-gitlog.git"
  },
  "keywords": [
    "git",
    "log",
    "parser"
  ],
  "author": "Dom Harrington <domharrington@protonmail.com>",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 20.x"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@auto-it/all-contributors": "^10.46.0",
    "@auto-it/first-time-contributor": "^10.46.0",
    "@auto-it/pr-body-labels": "^10.46.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.1",
    "auto": "^10.46.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "auto": {
    "plugins": [
      "npm",
      "released",
      "first-time-contributor",
      "all-contributors",
      "pr-body-labels"
    ]
  },
  "type": "module"
}
